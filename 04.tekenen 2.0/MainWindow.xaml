<Window x:Class="_04.tekenen_2._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_04.tekenen_2._0"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Name="mainWindow">
    <Grid Name="mainGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="413*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="104*"/>
        </Grid.ColumnDefinitions>
        <Button Name="Circle1" Content="Draw Circle1" HorizontalAlignment="Left" Margin="0,49,0,0" VerticalAlignment="Top" Width="88" Click="Circle_Click" Grid.Column="2" Height="20"/>
        <Button Name="Circle2" Content="Draw Circle2" Grid.Column="2" HorizontalAlignment="Left" Margin="0,83,0,0" VerticalAlignment="Top" Width="88" Click="Circle2_Click"/>
        <Button Name="Rectangle1" Content="Draw Rectangle" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top" Width="88" Click="Button_Click" Grid.Column="2" Height="20"/>
        <Button Name="Line1" Content="Draw Line" Grid.Column="2" HorizontalAlignment="Left" Margin="0,120,0,0" VerticalAlignment="Top" Width="88" Click="Line1_Click"/>

    </Grid>
</Window>
